[user]
	email = fryguy9@gmail.com
	name = Jason Frey
	signingkey = 
[color]
	ui = auto
	diff = auto
[core]
	excludesfile = /Users/jfrey/.gitignore_global
	pager = less -FX
	attributesfile = /Users/jfrey/.gitattributes
[merge]
	renamelimit = 99999
[github]
	user = Fryguy
	token = 
[credential]
	helper = osxkeychain
[pull]
	rebase = true
[push]
	default = simple
[status]
	short = true
	branch = true
[grep]
	lineNumber = true
[diff]
	algorithm = patience
[rebase]
	autosquash = true
[help]
	autocorrect = 1
[rerere]
	enabled = 1
[alias]
	lg = log --graph --pretty=format:\"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(cyan)<%an>%Creset\"
	ctags = !/Users/jfrey/.git_hooks/ctags
[log]
	follow = true
[commit]
	gpgsign = true
[branch]
	sort = -committerdate
